#define _CRT_SECURE_NO_WARNINGS 
#include<stdio.h>
int fibonacci(int a) {
	if (a == 1 || a == 2) {
		return 1;
	}
	int first = 1, second = 1, result = 0;
	for (int i = 3; i <= a; i++) {
		result = first + second;
		first = second;
		second = result;
	}
	return result;
}





int main() {
	int n;
	scanf("%d", &n);
	for (int i = 0; i < n; i++) {
		int a;
		scanf("%d", &a);
		printf("%d\n", fibonacci(a));
	}
	return 0;
}
